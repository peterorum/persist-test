{
  "name": "persist-test",
  "version": "1.0.0",
  "description": "Test various react things",
  "main": "index.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --hot --disable-host-check",
    "clean-build": "rimraf build/*",
    "build": "npm run clean-build && cross-env NODE_ENV=production webpack --mode production",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ignore-path .gitignore -- .",
    "lint:css": "stylelint './src/**/*.js'",
    "stats": "webpack --profile --json > stats.json && mv stats.json build && webpack-bundle-analyzer build/stats.json"
  },
  "author": "peter@peterorum.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peterorum/persist-test"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-styled-components": "^1.10.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-compat": "^3.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "immer": "^3.1.3",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "sanitize.css": "^10.0.0",
    "styled-components": "^4.3.1"
  }
}
